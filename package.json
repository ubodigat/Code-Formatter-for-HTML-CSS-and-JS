{
  "name": "ubodigat-code-formatter",
  "displayName": "U:Bodigat Code Formatter",
  "description": "A modern code formatter for HTML, CSS and JavaScript, based on Lonefy's original JS-CSS-HTML Formatter.",
  "version": "1.0.1",
  "publisher": "ubodigat",
  "author": "Originally by Lonefy, maintained and updated by U:Bodigat Development",
  "license": "MIT",
  "icon": "images/icon.png",
  "contributors": [
    {
      "name": "Lonefy",
      "url": "https://github.com/Lonefy"
    },
    {
      "name": "U:Bodigat",
      "url": "https://github.com/ubodigat"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ubodigat/Code-Formatter-for-HTML-CSS-and-JS"
  },
  "bugs": {
    "url": "https://github.com/ubodigat/Code-Formatter-for-HTML-CSS-and-JS/issues"
  },
  "homepage": "https://github.com/ubodigat/Code-Formatter-for-HTML-CSS-and-JS#readme",
  "engines": {
    "vscode": "^1.106.0"
  },
  "categories": [
    "Formatters",
    "Programming Languages"
  ],
  "keywords": [
    "formatter",
    "beautify",
    "html",
    "css",
    "javascript",
    "json",
    "prettify",
    "beautifier"
  ],
  "activationEvents": [
    "onLanguage:html",
    "onLanguage:css",
    "onLanguage:javascript",
    "onLanguage:json",
    "onLanguage:scss"
  ],
  "main": "./out/src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "formatter.beautify",
        "title": "Beautify Document"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package"
  },
  "dependencies": {
    "js-beautify": "^1.15.1",
    "json5": "^2.2.3",
    "mkdirp": "^3.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/vscode": "^1.106.0",
    "@vscode/vsce": "^3.7.1",
    "typescript": "^5.9.3"
  }
}
